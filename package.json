{
  "name": "@freispace/analytics-mcp-server",
  "version": "1.0.0",
  "description": "A Model Context Protocol server for analytical queries on the freispace platform.",
  "license": "MIT",
  "author": "freispace GmbH (https://freispace.com)",
  "homepage": "https://freispace.com",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cluster2a/freispace-analytics-mcp-server.git"
  },
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "bin": {
    "freispace-analytics-mcp-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector dist/index.js",
    "start": "node dist/index.js",
    "format": "biome format --write",
    "check": "biome check --write"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "mcp",
    "freispace",
    "analytics",
    "automation"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "zod": "3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@types/node": "^24.1.0",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.38.0"
  }
}
